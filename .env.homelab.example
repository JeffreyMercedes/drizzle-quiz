# Homelab Deployment Environment Variables
# Copy this file to .env on the homelab before deploying

# Database password - CHANGE THIS for production
DB_PASSWORD="your-secure-database-password-here"

# NextAuth Configuration
# Using Tailscale hostname for internal access
NEXTAUTH_URL="http://homelab:3100"

# Generate with: openssl rand -base64 32
NEXTAUTH_SECRET="your-secret-key-here-generate-with-openssl-rand-base64-32"
